<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xmlns:jaxws="http://cxf.apache.org/jaxws"
 xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans.xsd
      http://cxf.apache.org/jaxws 
      http://cxf.apache.org/schemas/jaxws.xsd"
      >
      <import resource="classpath:META-INF/cxf/cxf.xml"/>
    <import resource="classpath:META-INF/cxf/cxf-servlet.xml"/>

 	 <jaxws:client id="userWsClient" serviceClass="com.ijoy.service.IUserService" 
        address="http://localhost:8889/core/service/User">
       <jaxws:outInterceptors>
       	<bean class="com.shebao.interceptor.OutInterceptor"></bean>
       </jaxws:outInterceptors>
      </jaxws:client> 
      
      <jaxws:client id="loginInfoClient" serviceClass="com.ijoy.service.ILoginInfoService" 
        address="http://localhost:8889/core/service/LoginInfo">
      </jaxws:client>
     
	
</beans>  
 